[access "refs/*"]
    read = group rdo-infra/ansible-role-tripleo-ci-reproducer
    owner = group rdo-infra/ansible-role-tripleo-ci-reproducer
[access "refs/heads/*"]
    label-Code-Review = -2..+2 group rdo-infra/ansible-role-tripleo-ci-reproducer
    submit = group rdo-infra/ansible-role-tripleo-ci-reproducer
    read = group Registered Users
    read = group rdo-infra/ansible-role-tripleo-ci-reproducer
    label-Verified = -2..+2 group rdo-infra/ansible-role-tripleo-ci-reproducer
    label-Workflow = -1..+0 group Registered Users
    label-Workflow = -1..+1 group rdo-infra/ansible-role-tripleo-ci-reproducer
[access "refs/meta/config"]
    read = group Registered Users
    read = group rdo-infra/ansible-role-tripleo-ci-reproducer
[receive]
    requireChangeId = true
[submit]
    mergeContent = false
    action = rebase if necessary
[project]
        description = Tripleo CI zuul based reproducer
