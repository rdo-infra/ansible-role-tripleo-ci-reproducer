- tenant:
    name: tripleo-ci-reproducer
    source:
      gerrit:
        config-projects:
          - zuul-config
        untrusted-projects:
          - test1
          - test2
      git.openstack.org:
        untrusted-projects:
          - exclude:
            - pipeline
            - job
            - semaphore
            - project
            - project-template
            - nodeset
            - secret
            projects:
            - openstack-infra/project-config
          - exclude:
            - project
            - project-template
            - job
            projects:
            - openstack-infra/openstack-zuul-jobs
          - exclude:
            - project
            - project-template
            projects:
            - openstack-infra/zuul-jobs
            - openstack-infra/tripleo-ci
            - openstack/tripleo-quickstart
            - openstack/tripleo-quickstart-extras
            - openstack/tripleo-upgrade
            - openstack/browbeat
            - openstack/tripleo-ha-utils
            - openstack/tripleo-heat-templates
            - openstack/openstack-virtual-baremetal
            - openstack/openstack-ansible-os_tempest
            - openstack/ansible-role-python_venv_build
            - openstack/ansible-config_template
            - openstack/tripleo-common
            - openstack/python-tripleoclient
            - openstack/requirements
            - openstack/ansible-role-container-registry
            - openstack/kolla
            - openstack/tripleo-repos
            - openstack/heat-agents
            - openstack-dev/devstack
            - openstack-infra/devstack-gate
            - openstack/cinder
            - openstack/diskimage-builder
            - openstack/glance
            - openstack/horizon
            - openstack/keystone
            - openstack/kolla-ansible
            - openstack/neutron
            - openstack/nova
            - openstack/packstack
            - openstack/puppet-aodh
            - openstack/python-openstackclient
            - openstack/rpm-packaging
            - openstack/swift
      rdoproject.org:
        untrusted-projects:
          - exclude:
            - project
            - project-template
            projects:
            - rdo-jobs
            - rdo-infra/ci-config
            - rdo-infra/ansible-role-tripleo-ci-reproducer
            - centos-opstools/opstools-ansible
